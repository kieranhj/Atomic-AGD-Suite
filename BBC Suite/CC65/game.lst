0011FE  3               ;------------------------------------------------------
0011FE  3               ; Particle engine.
0011FE  3               ;
0011FE  3               ; Init particle data for 55 particles in SHRAPN table.
0011FE  3               ; Every particle has 6 bytes.
0011FE  3               ;
0011FE  3               ; global:	-
0011FE  3               ; local:	x,y,hl
0011FE  3               ; calls:	-
0011FE  3               ;------------------------------------------------------
0011FE  3               
0011FE  3               inishr:
0011FE  3               	lda #<SHRAPN 		; table.
0011FE  3               	sta z80_l
0011FE  3               	lda #>SHRAPN
0011FE  3               	sta z80_h
0011FE  3               
0011FE  3               	ldy #0
0011FE  3               	ldx #NUMSHR		; shrapnel pieces to process.
0011FE  3               inish0:
0011FE  3               	lda #255 		; kill the shrapnel.
0011FE  3               	sta (z80_hl),y
0011FE  3               
0011FE  3               	clc 			; point there.
0011FE  3               	lda z80_l
0011FE  3               	adc #SHRSIZ		; distance to next.
0011FE  3               	sta z80_l
0011FE  3               	bcc :+
0011FE  3               	inc z80_h
0011FE  3               :
0011FE  3               	dex
0011FE  3               	bne inish0 		; round again.
0011FE  3               	rts
0011FE  3               
0011FE  3               ;------------------------------------------------------
0011FE  3               ; Check for collision between laser and sprite.
0011FE  3               ;------------------------------------------------------
0011FE  3               
0011FE  3               lcol:
0011FE  3               	lda #<SHRAPN		; shrapnel table.
0011FE  3               	sta z80_l
0011FE  3               	lda #>SHRAPN
0011FE  3               	sta z80_h
0011FE  3               
0011FE  3               	lda #NUMSHR		; number of pieces in table.
0011FE  3               	sta z80_b
0011FE  3               lcol0:
0011FE  3               	ldy #0
0011FE  3               	lda (z80_hl),y 		; get type.
0011FE  3               	beq lcol1		; yes, check collision.
0011FE  3               lcol3:
0011FE  3               	clc			; point to more shrapnel.
0011FE  3               	lda z80_l
0011FE  3               	adc #SHRSIZ
0011FE  3               	sta z80_l
0011FE  3               	bcc :+
0011FE  3               	inc z80_h
0011FE  3               :
0011FE  3               	dec z80_b		; repeat for all shrapnel.
0011FE  3               	bne lcol0
0011FE  3               	rts 			; no collision, carry not set.
0011FE  3               lcol1:
0011FE  3               	ldy #3
0011FE  3               	lda (z80_hl),y		; get y.
0011FE  3               	sec
0011FE  3               	ldy #8
0011FE  3               	sbc (z80_ix),y		; subtract sprite y.
0011FE  3               lcolh:
0011FE  3               	cmp #16 		; within range?
0011FE  3               	bcc :+
0011FE  3               	jmp lcol2		; no, missed.
0011FE  3               :
0011FE  3               	ldy #5
0011FE  3               	lda (z80_hl),y 		; get x.
0011FE  3               	sec
0011FE  3               	ldy #9
0011FE  3               	sbc (z80_ix),y 		; subtract sprite y.
0011FE  3               	cmp #16			; within range?
0011FE  3               	bcs :+
0011FE  3               	jmp lcol4 		; yes, collision occurred.
0011FE  3               :
0011FE  3               lcol2:
0011FE  3               	jmp lcol3
0011FE  3               lcol4:
0011FE  3               	sec
0011FE  3               	rts 			; return with carry set for collision.
0011FE  3               .endif
