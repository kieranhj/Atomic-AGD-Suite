001146  3               	ldx #NUMSHR		; shrapnel pieces to process.
001146  3               inish0:
001146  3               	lda #255 		; kill the shrapnel.
001146  3               	sta (z80_hl),y
001146  3               
001146  3               	clc 			; point there.
001146  3               	lda z80_l
001146  3               	adc #SHRSIZ		; distance to next.
001146  3               	sta z80_l
001146  3               	bcc :+
001146  3               	inc z80_h
001146  3               :
001146  3               	dex
001146  3               	bne inish0 		; round again.
001146  3               	rts
