0011FE  3               	asl a  			; is this slot in use?
0011FE  3               	bcc :+
0011FE  3               	rts			; no, we can use it.
0011FE  3               :
0011FE  3               	clc			; point to more shrapnel.
0011FE  3               	lda z80_x
0011FE  3               	adc #SHRSIZ
0011FE  3               	sta z80_x
0011FE  3               	bcc :+
0011FE  3               	inc z80_i
