001163  3               inish0:
001163  3               	lda #255 		; kill the shrapnel.
001163  3               	sta (z80_hl),y
001163  3               
001163  3               	clc 			; point there.
001163  3               	lda z80_l
001163  3               	adc #SHRSIZ		; distance to next.
001163  3               	sta z80_l
001163  3               	bcc :+
001163  3               	inc z80_h
001163  3               :
001163  3               	dex
001163  3               	bne inish0 		; round again.
001163  3               	rts
