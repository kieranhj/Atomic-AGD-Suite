0011FE  3               	sta z80_l
0011FE  3               	lda #>shrsin
0011FE  3               	adc #0
0011FE  3               	sta z80_h
0011FE  3               
0011FE  3               	ldy #0
0011FE  3               	lda (z80_hl),y 		; fetch value from table.
0011FE  3               	sta z80_e
0011FE  3               	inc z80_l 		; next byte of table.
0011FE  3               	bne :+
0011FE  3               	inc z80_h
0011FE  3               :
0011FE  3               	ldy #0
0011FE  3               	lda (z80_hl),y		; fetch value from table.
0011FE  3               	sta z80_d
0011FE  3               	inc z80_l		; next byte of table.
0011FE  3               	bne :+
